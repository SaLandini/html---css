to_html_table<-function(tabela){
tags$table(
tags$thead(tags$tr(lapply(colnames(tabela), function(x) tags$th(x)))),
tags$tbody(
apply(dataframe,1, function(x) { tags$tr(lapply(x, function(y) tags$td(y)))})
))
}
View(to_html_table)
View(to_html_table)
